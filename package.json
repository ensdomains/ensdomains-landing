{
  "name": "ens.domains-v2",
  "private": true,
  "description": "ENS Domains homepage v2",
  "version": "0.2.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build:next": "next build",
    "build:assets": "tsx scripts/build-assets/index.ts",
    "build": "pnpm run build:assets && pnpm run build:next",
    "dev": "next dev --turbopack",
    "develop": "pnpm run dev",
    "start": "pnpm run dev",
    "lint": "eslint ./src ./scripts -c eslint.config.mjs",
    "lint:css": "stylelint -c stylelint.config.mjs './src/**/*.css'",
    "typecheck": "tsc --noEmit",
    "preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
    "deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
    "upload": "opennextjs-cloudflare build && opennextjs-cloudflare upload",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts"
  },
  "dependencies": {
    "@ensdomains/ensjs": "4.0.1-alpha.0",
    "@mdx-js/loader": "^3.1.0",
    "@mdx-js/react": "^3.1.0",
    "@next/mdx": "15.1.6",
    "@opennextjs/cloudflare": "^1.0.2",
    "@tanstack/react-query": "^5.64.2",
    "clsx": "^2.1.1",
    "dune-api-client": "^1.0.1",
    "i18next": "^23.16.0",
    "i18next-resources-to-backend": "^1.2.1",
    "next": "15.1.6",
    "react": "^19.0.0",
    "react-countup": "^6.5.3",
    "react-dom": "^19.0.0",
    "react-external-link": "^2.3.1",
    "react-i18next": "^15.0.3",
    "typescript": "^5.6.3",
    "viem": "^2.21.28",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint-react/eslint-plugin": "^1.25.0",
    "@eslint/compat": "^1.2.5",
    "@eslint/js": "^9.19.0",
    "@next/eslint-plugin-next": "15.1.6",
    "@shikijs/rehype": "^1.26.2",
    "@sindresorhus/slugify": "^2.2.1",
    "@stylistic/eslint-plugin": "^2.13.0",
    "@stylistic/stylelint-plugin": "^3.1.1",
    "@types/mdx": "^2.0.13",
    "@types/node": "^22.7.6",
    "@types/react": "^19",
    "acorn": "^8.14.0",
    "acorn-jsx": "^5.3.2",
    "deepmerge-ts": "^7.1.3",
    "eslint": "^9.19.0",
    "feed": "^4.2.2",
    "globals": "^15.11.0",
    "i18next-locales-sync": "^2.1.0",
    "mdast-util-to-string": "^4.0.0",
    "mdx-annotations": "^0.1.4",
    "reading-time": "^1.5.0",
    "remark-gfm": "^4.0.0",
    "schema-dts": "^1.1.2",
    "sharp": "^0.33.5",
    "stylelint": "^16.14.1",
    "ts-pattern": "^5.6.2",
    "tsx": "^4.19.2",
    "typescript-eslint": "^8.22.0",
    "unist-util-visit": "^5.0.0",
    "wrangler": "^4.14.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ensdomains/ensdomains-landing"
  },
  "bugs": {
    "url": "https://github.com/ensdomains/ensdomains-landing/issues"
  },
  "packageManager": "pnpm@9.3.0"
